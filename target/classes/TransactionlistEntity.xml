<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="walletmicroservice.Model.TransactionlistEntity">
        <table name="TRANSACTIONLIST" schema="walletmicroservice" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="11"/>
            </id>
            <basic name="amount">
                <column name="AMOUNT" precision="-1"/>
            </basic>
            <basic name="transactionType">
                <column name="TRANSACTION_TYPE"/>
            </basic>
            <basic name="transactionDate">
                <column name="TRANSACTION_DATE"/>
            </basic>
            <basic name="walletId">
                <column name="WALLET_ID" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
